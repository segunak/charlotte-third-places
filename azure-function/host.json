{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request;Dependency;Event;Exception"
      }
    },
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "Information",
      "Host.Aggregator": "Trace",
      "Host.Results": "Information",
      "Function": "Debug"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}